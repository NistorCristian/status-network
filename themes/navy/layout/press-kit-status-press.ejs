<section class="intro pt-150">
	<div class="container">
		<div class="row">
			<div class="col-md-10">
				<h1 class="mb-50 status-h1">
					<%- image_tag("/img/press-kit-status-logo.svg") %>
				</h1>
			</div>
		</div>
		<div class="row">
			<div class="col-xl-5 col-md-7">
				<p class="teaser-1 mb-150">Access the latest Press from Status and download any files for self publishing using our branded assets.</p>
				<a href="/press-kit/" class="link-arrow black">
					<%- image_tag("/img/arrow-back-black.svg") %>
					back to all projects
				</a>
			</div>
		</div>
	</div>
	<div class="floating-logo">
		<%- image_tag("/img/floating-logo.svg") %>
	</div>
</section>

<div class="brand-assets">
		<section class="quick-nav quick-nav-press-kit">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<ul>
						<li>
							<a href="/press-kit/status/">
								<%- image_tag("/img/icon-brand-assets.svg") %>
								Brand Assets						
							</a>
						</li>
						<li>
							<a href="/press-kit/status/press/" class="active">
								<%- image_tag("/img/icon-press-materials.svg") %>
								Press
							</a>
						</li>
						<li>
							<a href="mailto:press@status.im">
								<%- image_tag("/img/icon-contact-us.svg") %>
								Contact Us
							</a>
						</li>
					</ul>
				</div>
			</div>
		</div>
	</section>
	<section class="press-releases dark pt-200 pb-200">
		<div class="container">
			<div class="row">
				<div class="col-md-6">
					<div class="inner-start">
						<h2>Press releases</h2>
						<p class="teaser-2">Official Status announcements and press releases.</p>
					</div>
				</div>
			</div>
		</div>
		<div class="container pt-150">
			<div class="row">
				<div class="col-md-12">
					<div class="feed">
						<ul class="list-posts"></ul>
						<div class="d-flex justify-content-center">
							<a href="#" class="load-more btn btn-border-white">Load More</a>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
</div>
